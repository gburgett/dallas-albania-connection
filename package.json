    "request": "^2.88.0",
    "request-debug": "^0.2.0",
    "stopwords": "0.0.9",
    "tough-cookie-file-store": "^1.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "twilio": "^3.27.1",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@types/lodash": "^4.14.123",
    "@types/react-instantsearch": "^5.2.2",
    "ava": "^1.0.1"
  },
  "ava": {
    "files": [
      "src/lambda/**/*.test.ts"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "compileEnhancements": false,
    "extensions": [
      "ts"

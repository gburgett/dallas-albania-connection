    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.8",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-csv": "^2.1.17",
    "gatsby-transformer-remark": "^2.2.0",
    "globby": "^9.1.0",
    "googleapis": "^36.0.0",
    "lodash": "^4.17.13",
    "netlify-cms": "^2.9.4",
    "netlify-lambda": "^1.2.0",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "querystring": "^0.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
